{
  "name": "dingoapp",
  "version": "0.1.0",
  "author": "Doyeon Baek",
  "description": "A dingo beta",
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "rs": "set BROWSER=none && set PORT=3000 && yarn start",
    "es": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "cs": "node-sass sass/main.scss src/css/style.css -w",
    "dev": "concurrently \"yarn cs\" \"yarn es\" \"yarn rs\"",
    "electron-pack": "npm run-script build && electron-builder build -c.extraMetadata.main=build/electron.js"
  },
  "build": {
    "appId": "com.dotslab.dingo",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@babel/core": "7.16.7",
